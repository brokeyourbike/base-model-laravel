{
    "name": "brokeyourbike/base-models",
    "description": "Base models to extend in the app.",
    "keywords": [
        "brokeyourbike",
        "laravel",
        "model",
        "eloquent"
    ],
    "type": "library",
    "license": "MPL-2.0",
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\BaseModels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\BaseModels\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^8|^9",
        "brokeyourbike/uid-keys": "^0.1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "version": "0.3.1"
}
